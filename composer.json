{
    "name": "mulky-sulaiman/fortify-ui-livewire",
    "description": "Fortify-driven Laravel UI replacement with Livewire Support",
    "keywords": [
        "fortify",
        "fortify-ui",
        "livewire"
    ],
    "homepage": "https://github.com/mulky-sulaiman/fortify-ui-livewire",
    "license": "MIT",
    "authors": [
        {
            "name": "Mulky Sulaiman",
            "email": "mulky.sulaiman@gmail.com",
            "homepage": "https://github.com/mulky-sulaiman/",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/fortify": "^1.25.4",
        "livewire/livewire": "^3.6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.70.1",
        "orchestra/testbench": "^10.0.2",
        "phpunit/phpunit": "^11.5.10",
        "vimeo/psalm": "^6.8.8"
    },
    "autoload": {
        "psr-4": {
            "MulkySulaiman\\FortifyUILivewire\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MulkySulaiman\\FortifyUILivewire\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MulkySulaiman\\FortifyUILivewire\\FortifyUILivewireServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
